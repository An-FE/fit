{
  "name": "awesome",
  "version": "1.0.2",
  "description": "tb-component 贴吧组件化开发流程，致力于打造一套完全前端模块化、真正意义上的组件即插即用的生态圈。",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://gitlab.baidu.com/tb-component/awesome.git"
  },
  "scripts": {
    "server": "node server.js",
    "server-dev": "node server.js dev",
    "webpack-dev": "node webpack.js",
    "webpack-dist": "webpack --profile --display-modules --optimize-minimize --optimize-occurence-order --optimize-dedupe --config webpack.config.js",
    "cdn": "webpack -p --profile --config webpack.config.cdn.js",
    "start-old": "npm run submodule-init && npm run submodule-update && npm run demo-init && npm run babel-cli && npm run module-check && npm run cdn && npm run server & npm run webpack-dev",
    "update": "npm run babel-scripts && ./cli update && npm run demo-init",
    "start": "npm run cdn && npm run server-dev & npm run webpack-dev",
    "build": "npm run babel-scripts && npm run demo-init && npm run cdn && npm run webpack-dist",
    "push": "npm run babel-scripts && ./cli diffpub",
    "push-old": "./cli diffpub && git add -A && git commit -m \"quick push\" && git push",
    "babel-scripts": "babel scripts/ --out-dir scripts-babel/",
    "module-manage-update": "node scripts-babel/module-manage/index.js update",
    "module-manage-push": "node scripts-babel/module-manage/index.js push",
    "babel-cli": "babel scripts/cli -o ./cli && chmod +x ./cli",
    "demo-init": "node scripts-babel/auto-demo/index.js",
    "module-check": "./cli init",
    "submodule-init": "git submodule init",
    "submodule-update": "git submodule update",
    "alone": "webpack -p --config webpack.config.alone.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.2.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "format-json": "^1.0.3",
    "json-loader": "^0.5.3",
    "koa": "^1.1.2",
    "koa-static-cache": "^3.1.3",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.13",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "sass-loader": "^3.1.1",
    "semver": "^5.1.0",
    "style-loader": "^0.13.0",
    "text-loader": "0.0.1",
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.5",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "antd": "^0.10.4",
    "bootstrap": "^4.0.0-alpha.2",
    "classnames": "^2.2.1",
    "cropperjs": "^0.5.6",
    "css-path-loader": "^0.2.14",
    "dateformat": "^1.0.12",
    "extract-text-webpack-plugin": "^0.9.1",
    "font-awesome": "^4.5.0",
    "highlight": "^0.2.4",
    "highlight.js": "^8.9.1",
    "history": "^2.0.0",
    "html-path-loader": "^0.1.10",
    "humps": "^1.0.0",
    "jquery": "^2.1.4",
    "jquery-mockjax": "git+https://github.com/andycall/jquery-mockjax",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "md5": "^2.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.1",
    "platform": "^1.3.1",
    "react-date-range": "^0.1.8",
    "react-dom": "^0.14.7",
    "react-highlight": "^0.6.1",
    "react-infinite": "^0.7.3",
    "react-list": "^0.7.14",
    "react-native-web": "0.0.12",
    "react-redux": "^4.0.1",
    "react-router": "^2.0.0",
    "react-style": "^0.5.5",
    "react-tappable": "^0.8.0",
    "read-dir-files": "^0.1.1",
    "redux": "^3.1.6",
    "store2": "^2.3.2",
    "superagent": "^1.6.1",
    "superagent-mock": "^1.10.0"
  }
