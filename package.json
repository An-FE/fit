{
  "name": "awesome",
  "version": "1.1.5",
  "description": "fit-component 贴吧组件化开发流程，致力于打造一套完全前端模块化、真正意义上的组件即插即用的生态圈",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://gitlab.baidu.com/tb-component/awesome.git"
  },
  "typescript": {
    "definition": "fit.d.ts"
  },
  "scripts": {
    "server": "node server.js",
    "server-dev": "node server.js dev",
    "webpack-dev": "node webpack.js",
    "webpack-dist": "webpack --profile --display-modules --optimize-minimize --optimize-occurence-order --optimize-dedupe --config webpack.config.js",
    "cdn": "webpack -p --profile --config webpack.config.cdn.js",
    "update": "npm run babel-scripts && node scripts-babel/module-manage/index.js update && npm run demo-init",
    "start": "npm run demo-init && npm run cdn && npm run server-dev & npm run webpack-dev",
    "build": "npm run babel-scripts && npm run demo-init && npm run cdn && npm run webpack-dist",
    "push": "npm run babel-scripts && node scripts-babel/module-manage/index.js push",
    "babel-scripts": "babel scripts/ --out-dir scripts-babel/",
    "babel-clean-scripts": "rm -rf scripts-babel",
    "demo-init": "node scripts-babel/auto-demo/index.js",
    "submodule-init": "git submodule init",
    "submodule-update": "git submodule update",
    "alone": "webpack -p --config webpack.config.alone.js",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "test:dev": "npm run test -- --debug",
    "server-render-babel": "babel lib/ --out-dir lib-babel/"
  },
